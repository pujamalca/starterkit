    "components": {
        "schemas": {
            "CategoryResource": {
                "title": "Category",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 3
                    },
                    "name": {
                        "type": "string",
                        "example": "News"
                    },
                    "slug": {
                        "type": "string",
                        "example": "news"
                    },
                    "description": {
                        "type": "string",
                        "nullable": true
                    },
                    "image": {
                        "type": "string",
                        "nullable": true
                    },
                    "icon": {
                        "type": "string",
                        "nullable": true
                    },
                    "color": {
                        "type": "string",
                        "example": "#F59E0B",
                        "nullable": true
                    },
                    "sort_order": {
                        "type": "integer",
                        "example": 1
                    },
                    "is_featured": {
                        "type": "boolean"
                    },
                    "is_active": {
                        "type": "boolean"
                    },
                    "posts_count": {
                        "type": "integer",
                        "nullable": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "type": "object"
            },
            "CommentResource": {
                "title": "Comment",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 45
                    },
                    "content": {
                        "type": "string",
                        "example": "Komentar yang sangat membantu"
                    },
                    "is_approved": {
                        "type": "boolean",
                        "example": true
                    },
                    "guest_name": {
                        "type": "string",
                        "nullable": true
                    },
                    "guest_email": {
                        "type": "string",
                        "nullable": true
                    },
                    "parent_id": {
                        "type": "integer",
                        "nullable": true
                    },
                    "metadata": {
                        "type": "object",
                        "nullable": true
                    },
                    "commentable_type": {
                        "type": "string",
                        "example": "App\\\\Models\\\\Post"
                    },
                    "commentable_id": {
                        "type": "integer",
                        "example": 12
                    },
                    "replies_count": {
                        "type": "integer",
                        "nullable": true
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "user": {
                        "$ref": "#/components/schemas/UserResource"
                    }
                },
